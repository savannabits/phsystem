{
    "name": "savannabits/admin-translations",
    "description": "Translation manager for savannabits/admin-ui",
    "keywords": [
        "translations",
        "localizations",
        "manager",
        "admin",
        "cms",
        "laravel",
        "savannabits"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Samson Maosa",
            "email": "smaosa@strathmore.edu",
            "homepage": "https://www.savannabits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/container": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0",
        "savannabits/admin-ui": "^7.0",
        "savannabits/admin-listing": "^7.0",
        "savannabits/translatable": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~4.0",
        "phpunit/phpunit": "^8.5",
        "doctrine/dbal": "~2.5.13|^2.6",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Savannabits\\AdminTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Savannabits\\AdminTranslations\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Savannabits\\AdminTranslations\\AdminTranslationsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
